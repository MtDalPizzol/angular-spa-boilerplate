{
  "name": "angular-spa-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for writing Angular 1.x single page applications with ES6 and Webpack",
  "scripts": {
    "build": "rimraf dist && webpack --bail --progress --profile",
    "dev": "webpack-dev-server --history-api-fallback --inline --progress",
    "test": "karma start",
    "test-watch": "karma start --auto-watch --no-single-run",
    "start": "npm run dev",
    "deploy": "node server.js"
  },
  "main": "server.js",
  "author": "Matheus Dal'Pizzol",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.5.0",
    "angular-animate": "^1.5.0",
    "angular-aria": "^1.5.0",
    "angular-breadcrumb": "^0.4.1",
    "angular-material": "^1.0.6",
    "angular-material-data-table": "^0.9.14",
    "angular-messages": "^1.5.0",
    "angular-ui-notification": "^0.2.0",
    "angular-ui-router": "^0.2.18",
    "express": "^4.13.4",
    "jquery": "^2.2.1",
    "lodash": "^4.5.1",
    "mdi": "^1.4.57",
    "oclazyload": "^1.0.9",
    "perfect-scrollbar": "^0.6.10"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.0",
    "autoprefixer": "^6.0.3",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.1.18",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^2.7.1",
    "isparta-instrumenter-loader": "^1.0.0",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.14",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs2-launcher": "^0.5.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.24",
    "karma-webpack": "^1.7.0",
    "ng-annotate-webpack-plugin": "^0.1.3",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.4.2",
    "null-loader": "^0.1.1",
    "postcss-loader": "^0.8.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.1",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
